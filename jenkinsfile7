pipeline {
  environment {
    registry = 'saurabhgore65/saurabh-website-image'
    registryCredential = 'sagoredockerid'
}
  agent {
  docker {
    alwaysPull true
    image 'saurabhgore65/new-tag'
  }
}
stages {
       stage('pull Image') {
          steps{
             script{
                 docker.withRegistry( '', registryCredential ) {
               dockerImage.pull()  
                }
            }
      }
    }
  }
   }
