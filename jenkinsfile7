pipeline {
  environment {
    registry = 'saurabhgore65/new-tag'
    registryCredential = 'sagoredockerid'
}
  agent {
  docker {
    alwaysPull true
    image 'saurabhgore65/new-tag'
  }
}
stages {
       stage('Create a container') {
         steps{
       sh 'docker run -dit -p 8081:8080 --name new-container saurabhgore65/new-tag'
        }
  }
   }
}
