pipeline {
  environment {
    registry = 'saurabhgore65/suarabh-website-image'
    registryCredential = 'sagoredockerid'
    dockerImage = ''
}
  agent any
stages {
       stage('pull Image') {
          steps{
             script{
                 docker.withRegistry( '', registryCredential ) {
               dockerImage.pull()  
                }
            }
      }
    }
  }
   }
