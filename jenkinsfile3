pipeline {
agent { dockerfile true 
 }
     environment {
    registry = 'saurabhgore65/saurabh-website-image'
    registryCredential = 'sagoredockerid'
    dockerImage = ''
}
 stages {
        stage('compile'){
              steps{
          sh 'docker images'
         }
            }
         stage('Deploy Image') {
          steps{
             script{
                 docker.withRegistry( '', registryCredential ) {
               dockerImage.push()  
                }
            }
      }   
    }
  }
}      
