pipeline {
     environment {
    registry = 'saurabhgore65/saurabh-website-image'
    registryCredential = 'sagoredockerid'
    dockerImage = ''
}
}
 agent { dockerfile true 
 }
    
    stages {
        stage('compile'){
              steps{
           
               sh 'docker images'
         }
            }
         stage('Deploy Image') {
          steps{
             script{
                 docker.withRegistry( '', registryCredential ) {
               dockerImage.push()  
                }
            }
      }   
    }
  }
      
